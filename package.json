{
  "name": "notifier-node",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.10.2"
  },
  "scripts": {
    "start": "node ./bin/www",
    "test": "NODE_ENV=test APP_SECRET=keyboard-cat TWILIO_ACCOUNT_SID=my-twilio-account-sid TWILIO_AUTH_TOKEN=my-twilio-auth-token TWILIO_NUMBER=my-twilio-number TWILIO_NOTIFICATION_SERVICE_SID=my-twilio-notification-service-sid node_modules/mocha/bin/mocha spec --recursive",
    "jshint": "node_modules/jshint/bin/jshint ."
  },
  "description": "Notifier with Node.js/Express and Twilio",
  "author": {
    "name": "Agustin Camino",
    "email": "agustin.camino@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TwilioDevEd/notifier-node"
  },
  "keywords": [
    "node",
    "notifier",
    "twilio",
    "express"
  ],
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.4",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.14.0",
    "flash": "^1.1.0",
    "morgan": "~1.8.1",
    "npm": "^4.5.0",
    "pug": "^2.0.0-beta3",
    "q": "^1.4.1",
    "serve-favicon": "~2.4.2",
    "twilio": "~3.0.0-rc.16"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "jshint": "^2.9.2",
    "mocha": "^3.3.0",
    "mockery": "^2.0.0",
    "sinon": "^2.1.0",
    "supertest": "^6.1.2",
    "underscore": "^1.8.3"
  }
